<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600" width="800" height="600">
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1a237e" />
      <stop offset="100%" stop-color="#7c4dff" />
    </linearGradient>
    <linearGradient id="accent-gradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" stop-color="#7c4dff" />
      <stop offset="100%" stop-color="#00e5ff" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="url(#bg-gradient)" />
  
  <!-- Grid pattern -->
  <g stroke="rgba(255,255,255,0.1)" stroke-width="1">
    <path d="M0,100 H800 M0,200 H800 M0,300 H800 M0,400 H800 M0,500 H800" />
    <path d="M100,0 V600 M200,0 V600 M300,0 V600 M400,0 V600 M500,0 V600 M600,0 V600 M700,0 V600" />
  </g>
  
  <!-- Decorative circles -->
  <g>
    <circle cx="150" cy="150" r="40" fill="none" stroke="rgba(124,77,255,0.3)" stroke-width="2" />
    <circle cx="650" cy="450" r="50" fill="none" stroke="rgba(124,77,255,0.3)" stroke-width="2" />
    <circle cx="250" cy="450" r="30" fill="none" stroke="rgba(0,229,255,0.3)" stroke-width="2" />
    <circle cx="550" cy="150" r="35" fill="none" stroke="rgba(0,229,255,0.3)" stroke-width="2" />
  </g>
  
  <!-- WhatsApp icon -->
  <g transform="translate(400,250) scale(0.8)">
    <circle cx="0" cy="0" r="100" fill="#25D366" />
    <path d="M0,50 C-27.6,50 -50,27.6 -50,0 C-50,-27.6 -27.6,-50 0,-50 C27.6,-50 50,-27.6 50,0 C50,27.6 27.6,50 0,50 Z M25,-20 C25,-20 30,-25 30,-30 C30,-35 25,-40 20,-40 C15,-40 -25,0 -25,0 C-25,0 -30,20 -20,25 C-10,30 10,10 10,10 C10,10 20,5 25,-5 C30,-15 25,-20 25,-20 Z" fill="white" />
  </g>
  
  <!-- Education elements -->
  <g>
    <!-- Book -->
    <g transform="translate(200,350)">
      <path d="M-60,-40 L60,-40 L60,40 L-60,40 Z" fill="rgba(0,229,255,0.2)" stroke="#00e5ff" stroke-width="2" />
      <path d="M-50,-30 L50,-30 M-50,-15 L50,-15 M-50,0 L50,0 M-50,15 L50,15 M-50,30 L50,30" stroke="#00e5ff" stroke-width="1" />
    </g>
    
    <!-- Math symbols -->
    <g transform="translate(600,350)" fill="#00e5ff">
      <text x="-40" y="-20" font-family="Arial" font-size="40" font-weight="bold">+</text>
      <text x="0" y="-20" font-family="Arial" font-size="40" font-weight="bold">-</text>
      <text x="40" y="-20" font-family="Arial" font-size="40" font-weight="bold">×</text>
      <text x="-20" y="30" font-family="Arial" font-size="40" font-weight="bold">÷</text>
      <text x="20" y="30" font-family="Arial" font-size="40" font-weight="bold">=</text>
    </g>
    
    <!-- Graduation cap -->
    <g transform="translate(400,150)">
      <path d="M-80,0 L0,-40 L80,0 L0,40 Z" fill="rgba(124,77,255,0.3)" stroke="#7c4dff" stroke-width="2" />
      <path d="M0,40 L0,80" stroke="#7c4dff" stroke-width="2" />
      <circle cx="0" cy="-10" r="10" fill="#00e5ff" />
    </g>
  </g>
  
  <!-- Text -->
  <g font-family="Arial, sans-serif" fill="#ffffff">
    <text x="400" y="500" font-size="24" text-anchor="middle" font-weight="bold">教育系统WhatsApp集成</text>
    <text x="400" y="530" font-size="16" text-anchor="middle">数学与科学问题解答服务</text>
  </g>
</svg>