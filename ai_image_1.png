<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600" width="800" height="600">
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1a237e" />
      <stop offset="100%" stop-color="#7c4dff" />
    </linearGradient>
    <linearGradient id="accent-gradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" stop-color="#7c4dff" />
      <stop offset="100%" stop-color="#00e5ff" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="url(#bg-gradient)" />
  
  <!-- Circuit pattern -->
  <g stroke="rgba(255,255,255,0.1)" stroke-width="1">
    <path d="M0,100 H800 M0,200 H800 M0,300 H800 M0,400 H800 M0,500 H800" />
    <path d="M100,0 V600 M200,0 V600 M300,0 V600 M400,0 V600 M500,0 V600 M600,0 V600 M700,0 V600" />
  </g>
  
  <!-- Nodes -->
  <g>
    <circle cx="200" cy="200" r="15" fill="#00e5ff" />
    <circle cx="400" cy="150" r="10" fill="#00e5ff" />
    <circle cx="600" cy="250" r="12" fill="#00e5ff" />
    <circle cx="300" cy="400" r="14" fill="#00e5ff" />
    <circle cx="500" cy="350" r="8" fill="#00e5ff" />
    <circle cx="650" cy="450" r="10" fill="#00e5ff" />
  </g>
  
  <!-- Connection lines -->
  <g stroke="url(#accent-gradient)" stroke-width="2">
    <path d="M200,200 L400,150 L600,250 L650,450 L500,350 L300,400 Z" fill="none" />
  </g>
  
  <!-- Central brain icon -->
  <g transform="translate(400,300)">
    <circle cx="0" cy="0" r="80" fill="rgba(0,0,0,0.3)" />
    <path d="M-40,-30 L40,-30 L40,30 L-40,30 Z M-20,-50 L20,-50 L20,-30 L-20,-30 Z M-20,30 L20,30 L20,50 L-20,50 Z" fill="#00e5ff" />
    <path d="M-60,0 L-40,0 M60,0 L40,0 M0,-70 L0,-50 M0,70 L0,50" stroke="#00e5ff" stroke-width="4" />
    <circle cx="-20" cy="-10" r="5" fill="#ffffff" />
    <circle cx="20" cy="-10" r="5" fill="#ffffff" />
    <path d="M-15,15 C-10,25 10,25 15,15" stroke="#ffffff" stroke-width="3" fill="none" />
  </g>
  
  <!-- Text -->
  <g font-family="Arial, sans-serif" fill="#ffffff">
    <text x="400" y="500" font-size="24" text-anchor="middle" font-weight="bold">AI代理技术</text>
    <text x="400" y="530" font-size="16" text-anchor="middle">Share AI 定制化解决方案</text>
  </g>
  
  <!-- Decorative elements -->
  <g>
    <circle cx="100" cy="100" r="30" fill="none" stroke="rgba(124,77,255,0.3)" stroke-width="2" />
    <circle cx="700" cy="500" r="40" fill="none" stroke="rgba(124,77,255,0.3)" stroke-width="2" />
    <circle cx="200" cy="500" r="20" fill="none" stroke="rgba(0,229,255,0.3)" stroke-width="2" />
    <circle cx="600" cy="100" r="25" fill="none" stroke="rgba(0,229,255,0.3)" stroke-width="2" />
  </g>
</svg>